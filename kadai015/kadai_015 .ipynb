        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def _printinfo(self):\n",
        "        print(\"name:\" + self.name)\n",
        "        print(\"age:\" + str(self.age))\n",
        "\n",
        "human = Human(\"noguchi\", 25)\n",
        "\n",
        "human._printinfo()"
      ]
    }
